    "okanjo-app": "^2.1.0",
    "should": "^13.2.3",
    "@elastic/elasticsearch": "^6.8.0"
  },
  "peerDependencies": {
    "okanjo-app": ">= 2",
    "@elastic/elastichsearch": ">= 6"
